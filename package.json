{
  "name": "true-preloader",
  "version": "1.0.0",
  "description": "This plugin allows you to add various types of preloaders to your project, implementing this feature properly. The core size is almost 3Kb, plus the code for your preloader. Please refer to the README.md to explore the plugin's capabilities.",
  "main": "dist/preloader-core.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development rollup -c -w",
    "build": "NODE_ENV=production rollup -c",
    "minify": "node mimifyDemoPreloaders.js"
  },
  "keywords": [
    "Preloader",
    "True preloader",
    "Preloader core",
    "Simple preloader",
    "Light preloader",
    "Loading page"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "Aleksandr Zidyganov",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-alias": "5.0.0",
    "@rollup/plugin-terser": "0.4.3",
    "prettier": "2.8.8",
    "rollup": "3.25.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-livereload": "2.0.5",
    "rollup-plugin-minify-html-literals-v3": "1.3.3",
    "rollup-plugin-serve": "2.0.2",
    "url": "0.11.1"
  }
}
